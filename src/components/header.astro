<header class="header-wrapper header-1">
  <div class="container">
    <div class="row middle-bar justify-content-between align-items-center">
      <div class="col-xl-3 col-lg-12 col-6 text-lg-center text-xl-left">
        <div class="logo mb-lg-4 mb-xl-0 text-xl-left">
          <a href="/"><img src="/assets/img/logo.png" alt="logo" /></a>
        </div>
      </div>
      <div class="col-xl-9 col-lg-12 d-none d-lg-block">
        <div
          class="header-info-element ml-xl-5 ml-md-4 d-flex justify-content-between align-items-center"
        >
          <div class="single-info-element">
            <div class="icon">
              <i class="fal fa-map-marked-alt"></i>
            </div>
            <div class="text">
              <h5>Domicilio:</h5>
              <span>Calle Lezo 8, 4D <br />28041 Madrid</span>
            </div>
          </div>
          <div class="single-info-element">
            <div class="icon">
              <i class="fal fa-clock"></i>
            </div>
            <div class="text">
              <h5>Horario:</h5>
              <span>24 HORAS</span>
            </div>
          </div>
          <div class="single-info-element">
            <div class="icon">
