---
import Bannerestrecho from "../../components/bannerestrecho.astro";
import Footer from "../../components/footer.astro";
import Header from "../../components/header.astro";
import Preloader from "../../components/preloader.astro";
import ServiceDetails from "../../components/service-details.astro";
import Topbarheader from "../../components/topheader.astro";
import Layout from "../../layout/Layout.astro";
import { frontmatter } from "../../data/madrid.md";


---

<Layout
titleSEO={frontmatter.metaTitle}
  descriptionMeta={frontmatter.metaDescription}
>
  <Preloader />
  <Topbarheader />
  <Header />
  <Bannerestrecho
    bannerTitle={frontmatter.bannerTitle}
    bannerRuta={frontmatter.bannerRuta}
  />
  <ServiceDetails
    title={frontmatter.title}
    aboutText={frontmatter.aboutText}
    conclusion={frontmatter.conclusion}
    content={frontmatter.content}
    servicios={frontmatter.servicios}
    title2={frontmatter.title2}
    text2={frontmatter.text2}
    intro={frontmatter.intro}
    _html={frontmatter._html}
    blockquote={frontmatter.blockquote}
    faq1={frontmatter.faq1}
    faq2={frontmatter.faq2}
    faq3={frontmatter.faq3}
    faq4={frontmatter.faq4}
    answer1={frontmatter.answer1}
    answer2={frontmatter.answer2}
    answer3={frontmatter.answer3}
    answer4={frontmatter.answer4}
    titleConclusion = {frontmatter.titleConclusion}
    titleFaqs = {frontmatter.titleFaqs}
    conclusion2 = {frontmatter.conclusion2}
    image1 = {frontmatter.image1}
    image2 = {frontmatter.image2}
    image3 = {frontmatter.image3}
    image4 = {frontmatter.image4}
  />

  <Footer />
</Layout>