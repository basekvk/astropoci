---
import Bannerestrecho from "../components/bannerestrecho.astro";
import Contact from "../components/contact.astro";
import Footer from "../components/footer.astro";
import Header from "../components/header.astro";
import Preloader from "../components/preloader.astro";
import Topbarheader from "../components/topheader.astro";
import Layout from "../layout/Layout.astro";
import { frontmatter } from "../data/contacto/contacto.md";
import FormularioContacto from "../components/Contacto.jsx";

---

<Layout
  titleSEO={frontmatter.metaTitle}
  descriptionMeta={frontmatter.metaDescription}
>
  <Preloader />
  <Topbarheader />
  <Header />
  <Bannerestrecho
  bannerTitle={frontmatter.bannerTitle}
  bannerRuta={frontmatter.bannerRuta}
/>
  
  <FormularioContacto client:load />
  <Footer />
</Layout>
